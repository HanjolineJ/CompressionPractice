# Install native compressors, only once
# macOS
brew install gzip bzip2 xz zstd lz4

# Ubuntu
sudo apt-get install -y gzip bzip2 xz-utils zstd lz4

# See what files sizes you have 
ls -lh GameAsset.txt LargeDataset.txt sample_files.txt

# Install depencdency 
 pip install psutil

# Run script
python script.py